section.section-results.section
	.wrapper
		.section-body
			h2.section-title.title.title-2.title-line= self.results.title[curlang]

			p.section-desc= self.results.desc[curlang]

			ul.results-list
				each item, key in self.results
					if key.startsWith('item')
						li.results-item(class=`icon-results-${key}`)
							p.results-item-subtitle.subtitle!= item.title[curlang]
							p.results-item-desc.title.title-2!= item.desc[curlang]
