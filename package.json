{
	"name": "ok-build",
	"version": "1.0.0",
	"description": "",
	"main": "gulpfile.js",
	"type": "module",
	"scripts": {
		"dev": "gulp",
		"release": "gulp release"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/darv86/webdesignok.git"
	},
	"author": "Dima Radushev <86darv@gmail.com>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/darv86/webdesignok/issues"
	},
	"homepage": "https://github.com/darv86/webdesignok#readme",
	"browserslist": [
		"> .5%"
	],
	"babel": {
		"presets": [
			"@babel/preset-env"
		]
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 4,
		"printWidth": 100,
		"singleQuote": true,
		"arrowParens": "avoid",
		"pugAttributeSeparator": "as-needed",
		"pugIdNotation": "as-is",
		"plugins": [
			"@prettier/plugin-pug"
		],
		"overrides": [
			{
				"files": "gulpfile.js",
				"options": {
					"printWidth": 1000
				}
			},
			{
				"files": "_reset.scss",
				"options": {
					"requirePragma": true
				}
			}
		]
	},
	"devDependencies": {
		"@babel/core": "^7.28.5",
		"@babel/preset-env": "^7.28.5",
		"@prettier/plugin-pug": "^3.4.2",
		"babel-loader": "^10.0.0",
		"browser-sync": "^3.0.4",
		"csvtojson": "^2.0.14",
		"gulp": "^5.0.1",
		"gulp-group-css-media-queries": "^1.2.2",
		"gulp-if": "^3.0.0",
		"gulp-imagemin": "^9.2.0",
		"gulp-pug": "^5.0.0",
		"gulp-real-favicon": "^0.3.2",
		"gulp-sass": "^6.0.1",
		"gulp-ttf2woff2": "^5.0.0",
		"gulp-webp": "^5.1.0",
		"gulp-webp-css-fixed": "^1.1.0",
		"gulp-webp-html-nosvg": "^1.1.1",
		"lodash": "^4.17.21",
		"sass": "^1.97.1",
		"vinyl-ftp": "^0.6.1",
		"vinyl-named": "^1.1.0",
		"webpack-stream": "^7.0.0"
	},
	"dependencies": {
		"@tannerhodges/snap-slider": "^2.0.3",
		"core-js-pure": "^3.47.0",
		"photoswipe": "^5.4.4"
	}
}
